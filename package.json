{
  "name": "portfolio-reccos",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "coverage:open": "vitest run --coverage && start coverage/index.html",
    "coverage:summary": "vitest run --coverage --reporter=verbose",
